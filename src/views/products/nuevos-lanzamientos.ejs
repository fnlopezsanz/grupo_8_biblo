<!DOCTYPE html>
<html lang="en">

<%- include("../parcials/head", {stylesheet: ["stylehome", "nl-b"]}) %>

    <body>
        <%- include("../parcials/header"); %>
        <main>
          <h1 class="title-h1">Nuevos Lanzamientos</h1>
            <div class="box-ebooks">
                <% for(let i=0; i < nuevosLanzamientos.length; i++){ %>
                    <article>
                      <div class="ebook">
                        <a href="/products/<%= nuevosLanzamientos[i].id %>"><img src="/img/products/<%= nuevosLanzamientos[i].imagen %>"
                            alt="<%= nuevosLanzamientos[i].titulo %>" class="imagen-ebook"></a>
                        <p class="ebook-title">
                            <%= nuevosLanzamientos[i].titulo %>
                        </p>
                      </div>
                    </article>
                    <% } %>
              </div>
        </main>
            <%- include("../parcials/footer"); %>
    </body>

</html>